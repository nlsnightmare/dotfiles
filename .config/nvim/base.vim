filetype on
syntax on
set expandtab
set tabstop=4
set softtabstop=4
set shiftwidth=4
set number relativenumber
set showcmd
filetype indent on
set smartcase
set ignorecase
set incsearch
set nohlsearch
set foldenable
set foldlevelstart=10
set foldmethod=indent
set completeopt=menu,menuone,longest,noinsert
set nowrap
set clipboard=unnamedplus
set notermguicolors
